#!/usr/bin/env php
<?php

require __DIR__.'/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use YamlDiff\ArgvInput;
use YamlDiff\YamlDiffCommand;

$cli = new Application('YamlDiff', YamlDiffCommand::VERSION);
$cli->add(new YamlDiffCommand);
$cli->run(new ArgvInput('yamldiff'));
